        <activity android:name=".view.login.KaoShiType_Activity"
            android:launchMode="singleTask"/>
        <activity android:name=".view.login.WangJiZhangHao_Activity" />
        <activity android:name=".view.login.ZhuCeZhangHao_Activity" />
        <activity android:name=".view.login.ZhangHaoLogin_Activity" />
        <activity android:name=".view.login.YanZhengLogin_Activity" />
        <activity android:name=".view.ShouYe_Activity"
            android:launchMode="singleTask"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>